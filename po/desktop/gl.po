msgstr "Icona da área de notificación para comprobar se hai actualizacións"

#: ./share/applications/org.mageia.dnfdragora-updater.desktop
msgctxt "Comment"
msgid "Automatically check for updates and notify"
msgstr "Comprobar automaticamente se hai actualizacións e notificar"

#: ./share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "Name"
msgid "dnfdragora Package Installer"
msgstr "Instalador de paquetes dnfdragora"

#: ./share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "GenericName"
msgid "RPM Package Installer"
msgstr "Instalador de paquetes RPM"

