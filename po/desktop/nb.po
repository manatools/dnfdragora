#: share/applications/org.mageia.dnfdragora.desktop
msgctxt "Name"
msgid "dnfdragora"
msgstr "dnfdragora"

#: share/applications/org.mageia.dnfdragora.desktop
msgctxt "GenericName"
msgid "Install & Remove Software"
msgstr "Installer og fjern programvare"

#: share/applications/org.mageia.dnfdragora.desktop
msgctxt "Comment"
msgid "A graphical front end for installing, removing and updating packages"
msgstr ""
"Grafisk grensesnitt for installering, fjerning og oppdatering av pakker."

#: share/applications/org.mageia.dnfdragora-updater.desktop
msgctxt "Name"
msgid "dnfdragora-updater"
msgstr "dnfdragora oppdaterer"

#: share/applications/org.mageia.dnfdragora-updater.desktop
msgctxt "GenericName"
msgid "Tray-icon to check for updates"
msgstr "Dokk ikon for sjekking av oppdateringer"

#: share/applications/org.mageia.dnfdragora-updater.desktop
msgctxt "Comment"
msgid "Automatically check for updates and notify"
msgstr "Automatisk sjekk for oppdateringer med varsel"

#: share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "Name"
msgid "dnfdragora Package Installer"
msgstr "dnfdragora installerer"

#: share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "GenericName"
msgid "RPM Package Installer"
msgstr "RPM pakke installerer"

#: share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "Comment"
msgid "Install local packages on the system"
msgstr "Installer lokal pakke på systemet"
